{
  "name": "chrome-extension-starter",
  "version": "0.0.0",
  "scripts": {
    "cz": "git-cz",
    "prepare": "husky install",
    "postinstall": "ngcc --properties es2015 browser module main",
    "affected:lint": "nx affected:lint",
    "start:dev": "nx run-many --target=build --all --configuration development --watch",
    "start:prod": "nx run-many --target=build --all --configuration production --watch",
    "build:dev": "nx run-many --target=build --all --configuration development",
    "build:prod": "nx run-many --target=build --all --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.1.0",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/language-service": "~13.1.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nrwl/angular": "13.3.8",
    "@nrwl/cli": "13.3.8",
    "@nrwl/eslint-plugin-nx": "13.3.8",
    "@nrwl/jest": "13.3.8",
    "@nrwl/js": "13.3.8",
    "@nrwl/linter": "13.3.8",
    "@nrwl/tao": "13.3.8",
    "@nrwl/workspace": "13.3.8",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/chrome": "^0.0.186",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "commitizen": "^4.2.4",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.3.1",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "prettier-plugin-rational-order": "^1.0.3",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
